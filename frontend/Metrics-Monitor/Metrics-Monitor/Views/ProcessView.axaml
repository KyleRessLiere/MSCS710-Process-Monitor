<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MetricsMonitorClient.Views.ProcessView">
	<UserControl.Resources></UserControl.Resources>
	
	<Grid Classes="ContentGrid">
		<Grid.RowDefinitions>
			<RowDefinition Height="70"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>
		<TextBlock Classes="h5" HorizontalAlignment="Center" Grid.Row="0">Active Processes</TextBlock>
	<TreeDataGrid Grid.Row="1" Height="520" VerticalAlignment="Bottom" Foreground="{DynamicResource MM-Light}" Background="DarkGray" Source="{Binding TreeGridDataSource}"/>

	</Grid>
	
	
</UserControl>
